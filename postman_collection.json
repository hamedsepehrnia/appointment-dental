      "value": "http://localhost:4000/api",
      "type": "string"
    },
    {
      "key": "csrfToken",
      "value": "",
      "type": "string"
    }
  ],
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Ø¯Ø±ÛŒØ§ÙØª Ø®ÙˆØ¯Ú©Ø§Ø± CSRF Token",
          "if (!pm.variables.get('csrfToken')) {",
          "    pm.sendRequest({",
          "        url: pm.variables.get('baseUrl') + '/csrf-token',",
          "        method: 'GET',",
          "        header: {}",
          "    }, function (err, res) {",
          "        if (!err && res.json().success) {",
          "            pm.variables.set('csrfToken', res.json().data.csrfToken);",
          "        }",
          "    });",
          "}"
        ]
      }
    }
  ],
